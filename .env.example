# NFT Auction Deployment Configuration
# Copy this to .env and fill in your values

# Network Configuration
NETWORK=testnet
RPC_ENDPOINT=https://rpc.testnet.midnight.network

# Local Node Configuration
NODE_URL=http://localhost:9944

# Deployer Configuration
DEPLOYER_ADDRESS=0x0000000000000000000000000000000000000000
DEPLOYER_PRIVATE_KEY=your_private_key_here

# Contract Configuration
CONTRACT_NAME=NFTAuction
CONTRACT_VERSION=1.0.0
CONTRACT_ADDRESS=your_deployed_contract_address_here

# Wallet Configuration (for backend operations)
WALLET_SEED=your_wallet_seed_phrase_here
SEED_PHRASE=your_wallet_seed_phrase_here
PRIVATE_KEY=your_private_key_here

# Deployment Options
SKIP_CONFIRMATION=false
VERBOSE=true
